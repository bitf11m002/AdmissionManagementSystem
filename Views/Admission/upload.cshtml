@model TermProject2.Models.student
 @{
    Layout = "~/Views/Shared/_Layout2.cshtml";
}

  
<div class="row">
<div id="sidebar" class="span3">
</div>

	<div class="span9">
    <ul class="breadcrumb">
		<li class="active">Online Admission<span class="divider">/</span></li>
		<li class="active">Upload Document</li>
    </ul>
	<h3>Upload Document</h3>	
	<hr class="soft"/>
	<div class="well">
	<div>	 </div>
<form class="form-horizontal" action="/Admission/upload2" method="post" enctype="multipart/form-data" >
            <div class="control-group">
                <label class="control-label" for="dob">Colored Copy of Recent Result<sup>*</sup></label>
                <div class="controls">
                    <input type="file" id="file1" name="file" required>
                </div>
            </div>

            
            <div class="control-group">
                <div class="controls">
                    <input type="hidden" name="email_create" value="1">
                    <input type="hidden" name="is_new_customer" value="1">
                    <input class="btn btn-large" type="submit" value="Upload" id="sub" />
                </div>
               
            </div>
        </form>
        
</div>

</div>
</div>  
